# HomeWizard Lite API Configuration
# Copy this file to .env and fill in your actual values

# HomeWizard Lite Account Credentials (REQUIRED)
HWL_USERNAME=your_homewizard_email@example.com
HWL_PASSWORD=your_homewizard_password

# Smart Plug Configuration (REQUIRED)
# Get this ID by calling /api/smartplug endpoint after setting up credentials
SMARTPLUG_ID=your_main_smartplug_id

# Server Configuration (OPTIONAL)
PORT=3033
MIN_DIMMING_VALUE=1
CACHE_TTL=1800
LOGLEVEL=warn